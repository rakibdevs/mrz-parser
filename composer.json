{
    "name": "rakibdevs/mrz-parser",
    "description": "A PHP package for MRZ (Machine Readable Zones) code parser for Passport, Visa & Travel Documents.",
    "keywords": [
        "rakibdevs",
        "mrz-parser"
    ],
    "homepage": "https://github.com/rakibdevs/mrz-parser",
    "license": "MIT",
    "authors": [
        {
            "name": "Md. Rakibul Islam",
            "email": "rakib1708@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Rakibdevs\\MrzParser\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rakibdevs\\MrzParser\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
